{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Project Debugger (launch.json)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/app/main.py",
      "console": "integratedTerminal",
      "autoreload": "true",
      "justMyCode": "true",
      "env": {
        "PYTHONPATH": ".venv/bin/python"
      }
    }
  ]
}
